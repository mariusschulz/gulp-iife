{
  "name": "gulp-iife",
  "version": "0.4.0",
  "description": "Wraps JavaScript code within an immediately invoked function expression.",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prepublish": "npm test && npm run build",
    "build": "cp -R ./src/ ./lib",
    "format": "prettier './*.js' 'src/**/*.js' 'test/**/*.js' --write",
    "test": "mocha test/transpiled",
    "test-watch": "mocha test/transpiled --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariusschulz/gulp-iife.git"
  },
  "keywords": [
    "gulpplugin",
    "javascript",
    "ecmascript",
    "js",
    "iife",
    "siaf",
    "immediately",
    "invoked",
    "function",
    "expression"
  ],
  "author": {
    "name": "Marius Schulz",
    "email": "marius.schulz@me.com",
    "url": "https://mariusschulz.com/"
  },
  "homepage": "https://github.com/mariusschulz/gulp-iife",
  "dependencies": {
    "lodash": "^4.17.11",
    "source-map": "^0.5.3",
    "through2": "^2.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "chai": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.0.0",
    "mocha": "^2.3.0",
    "prettier": "^2.0.5"
  }
}
